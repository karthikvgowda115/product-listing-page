{
  "name": "innerwear-shop-backend",
  "version": "1.1.0",
  "description": "Backend API for Premium Innerwear E-commerce Store",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seeders/seed-innerwear-products.js",
    "test": "jest",
    "lint": "eslint .",
    "migrate": "node database/migrations/create-tables.js",
    "reset": "node database/migrations/reset-database.js"
  },
  "keywords": [
    "innerwear",
    "ecommerce",
    "nodejs",
    "express",
    "mysql",
    "backend",
    "api"
  ],
  "author": "Innerwear Shop Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/innerwear-shop-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/innerwear-shop-backend/issues"
  },
  "homepage": "https://github.com/yourusername/innerwear-shop-backend#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.0",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.8",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.2",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "services/**/*.js",
      "middlewares/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
